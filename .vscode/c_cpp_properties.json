{
    "env": {
        "myIncludePath": [
            "${workspaceFolder}/include",
            "${workspaceFolder}/src"
        ]
    },
    "configurations": [
        {
            "name": "Linux",
            "compilerPath": "/usr/bin/c99",
            "includePath": [
                "${workspaceFolder}/include",
                "${workspaceFolder}/src"
            ],
            "cStandard": "c99",
            "configurationProvider": "none",
            "mergeConfigurations": true,
            "defines": [
                "SAW_DEBUG_MODE"
            ]
        }
    ],
    "version": 4,
    "enableConfigurationSquiggles": true
}